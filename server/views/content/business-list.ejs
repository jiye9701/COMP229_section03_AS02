 <div class="row justify-content-md-center">
          <div class="col">

            <a href="/business-list/add" class="btn btn-primary btn-lg"><i class="fas fa-plus-circle"></i> Add New Component</a>
            <hr>

            <table class="table table-striped table-bordered table-hover">
              <thead class="thead-dark">
                <tr>
                  <th scope="col">Contact Name</th>
                  <th scope="col">Contact Number</th>
                  <th scope="col">Email Address</th>

                  <th></th>
                  <th></th>
                </tr>
              </thead>
              <tbody>

                <% for(let count=0; count < business.length; count++) { %>

                <tr>
                  <td><%= business[count].name %></td>
                  <td><%= business[count].contactNumbet %></td>
                  <td><%= business[count].email %></td>

                  <td>
                    <a href="/business-list/edit/<%= business[count]._id %>" class="btn btn-primary btn-sm">
                        <i class="fas fa-edit"></i> Edit</a>
                  </td>
                  <td>
                      <a href="/business-list/delete/<%= business[count]._id %>" class="btn btn-danger btn-sm">
                          <i class="fas fa-trash-alt"></i> Delete</a>
                  </td>
                </tr>
                
              <% } %>

              </tbody>
            </table>

          </div>
        </div>